<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyTitleTextStyle" parent="TextAppearance.MaterialComponents.Headline6">
        <item name="android:textSize">22sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">@font/ys_display_bold</item>
    </style>
    <style name="ButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColor">@color/buttonTextColor</item>
        <item name="android:textSize">@dimen/textSize_01</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/iconPadding</item>
        <item name="iconTint">@color/iconColor</item>
        <item name="cornerRadius">@dimen/cornerRadius</item>
        <item name="android:fontFamily">@font/ys_display_heavy</item>
    </style>
    <style name="MaterialTextViewStyle">
        <item name="android:paddingVertical">@dimen/marginTop</item>
        <item name="android:textColor">@color/text_light</item>
        <item name="android:textSize">@dimen/textSize_01</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
</resources>